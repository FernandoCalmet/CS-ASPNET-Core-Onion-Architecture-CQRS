{
  "ConnectionStrings": {
    "Database": "Host=mycompany-myproduct-db;Port=5432;Database=mycompany-myproduct;Username=postgres;Password=postgres;",
    "Cache": "mycompany-myproduct-redis:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://mycompany-myproduct-seq:5341" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  }
}
